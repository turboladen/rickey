<div class="col-md-12">
  <h2>Recipes</h2>

  <table class="table table-striped">
    <thead>
      <tr>
        <th>Title</th>
        <th>Created</th>
      </tr>
    </thead>
    <tbody>
      {{#each model as |recipe|}}
        <tr>
          <td>{{#link-to "recipe" recipe}}{{recipe.title}}{{/link-to}}</td>
            <td>{{recipe.createdAt}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>

{{outlet}}
